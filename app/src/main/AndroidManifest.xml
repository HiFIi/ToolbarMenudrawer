<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kyler.toolbarmenudrawer"
    android:versionCode="10"
    android:versionName="0.0.4.3">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_launcher_new"
        android:label="@string/app_name"
        android:theme="@style/ToolbarMenudrawer.Theme">
        <activity
            android:name="com.kyler.toolbarmenudrawer.ToolbarMenudrawer"
            android:label="@string/app_name"
            android:theme="@style/ToolbarMenudrawer.Theme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kyler.toolbarmenudrawer.activities.FirstRun"
            android:label="@string/blank_text"
            android:theme="@style/WhiteTeal.Theme"></activity>
    </application>

</manifest>
